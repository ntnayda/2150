#ifndef STACK_H
#define STACK_H

using namespace std;

class Stack {

 public:
  Stack(); //constructor
  push(); //adds a number to the top of the stack
  pop(); //removes the number at the top of the stack
  top(); //returns the number at the top of the stack
  empty(); //clears the stack
  friend class StackNode,StackItr;

 private:
  StackItr() sItr;
  StackNode() * head;

};

#endif
