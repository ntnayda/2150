#ifndef POSTFIXCALCULATOR_H
#define POSTFIXCALCULATOR_H

#include <iostream>
#include <stack>
using namespace std;

class postfixCalculator {

 Public:

  void push(int e);
  int top() const;
  void pop();
  bool empty() const;
  void add();
  void subtract();
  void multiply();
  void divide();
  void negate();

 Private:
  stack<int> s;
};


#endif
